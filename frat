local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
local Window = Rayfield:CreateWindow({
   Name = "Куни HUB",
   Icon = 0, -- Icon in Topbar. Can use Lucide Icons (string) or Roblox Image (number). 0 to use no icon (default).
   LoadingTitle = "Rayfield Interface Suite",
   LoadingSubtitle = "by Куни",
   ShowText = "Rayfield", -- for mobile users to unhide rayfield, change if you'd like
   Theme = "Default", -- Check https://docs.sirius.menu/rayfield/configuration/themes

   ToggleUIKeybind = "K", -- The keybind to toggle the UI visibility (string like "K" or Enum.KeyCode)

   DisableRayfieldPrompts = false,
   DisableBuildWarnings = false, -- Prevents Rayfield from warning when the script has a version mismatch with the interface

   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil, -- Create a custom folder for your hub/game
      FileName = "Big Hub"
   },

   Discord = {
      Enabled = false, -- Prompt the user to join your Discord server if their executor supports it
      Invite = "noinvitelink", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ ABCD would be ABCD
      RememberJoins = true -- Set this to false to make them join the discord every time they load it up
   },

   KeySystem = false, -- Set this to true to use our key system
   KeySettings = {
      Title = "Untitled",
      Subtitle = "Key System",
      Note = "No method of obtaining the key is provided", -- Use this to tell the user how to get a key
      FileName = "Key", -- It is recommended to use something unique as other scripts using Rayfield may overwrite your key file
      SaveKey = true, -- The user's key will be saved, but if you change the key, they will be unable to use your script
      GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
      Key = {"Hello"} -- List of keys that will be accepted by the system, can be RAW file links (pastebin, github etc) or simple strings ("hello","key22")
   }
})
local Tab = Window:CreateTab("Куни блоб", "rewind")
local function GetPlayers()
        local players = game:GetService("Players"):GetPlayers()
        local playerNames = {}
        
        for _, player in ipairs(players) do
            table.insert(playerNames, player.Name.." ".." ("..player.DisplayName..")")

        end
return playerNames
    end
	local selectedPlrName
local Dropdown1 = Tab:CreateDropdown({
   Name = "target",
   Options = GetPlayers(),
   CurrentOption = {nil},
   MultipleOptions = false,
   Flag = "Dropdown1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Options)
local e2 = tostring(Options[1])
local e1 = string.split(e2, " ")
local e = e1[1]
    selectedPlrName = e
end,
})
local plr = game.Players.LocalPlayer 
local char = plr.Character or plr.CharacterAdded:Wait()
local hum = char:WaitForChild("Humanoid")
local function FindBlob()
    local seat = hum.SeatPart
    if seat then
        return seat:FindFirstAncestor("CreatureBlobman")
    end
    return nil
end 
local loopkick
local Toggle = Tab:CreateToggle({
   Name = "блоб килл[таргет]",
   CurrentValue = false,
   Flag = "Toggle1",
   Callback = function(Value)
        loopkick = Value
        local connection
        
        local function kickPlayer()
            local plr = game.Players:FindFirstChild(selectedPlrName)
            if not plr then return end
            
            -- Ждем пока игрок появится или возродится
            if not plr.Character then
                plr.CharacterAdded:Wait()
                task.wait(1) -- Ждем полного появления персонажа
            end
            
            local hrp = plr.Character:FindFirstChild("HumanoidRootPart")
            if not hrp then return end
            local blob = FindBlob()
            if not blob then return end
            
            -- 1. Телепортируемся к игроку
            local myCharacter = game.Players.LocalPlayer.Character
            if myCharacter then
                local myHrp = myCharacter:FindFirstChild("HumanoidRootPart")
                if myHrp then
                    myHrp.CFrame = hrp.CFrame * CFrame.new(0, 3, 0) -- телепорт к игроку +3 studs выше
                end
            end
            
            -- Ждем 0.2 секунды перед Grab
            task.wait(0.2)
            
            -- 2. Grab игрока
            blob.BlobmanSeatAndOwnerScript.CreatureGrab:FireServer(
                blob.LeftDetector,
                hrp, 
                blob.LeftDetector.LeftWeld
            )
            
            -- Ждем немного чтобы граб зарегистрировался
            task.wait(0.3)
            
            -- 3. Телепорт ниже уровня уничтожения
            workspace.FallenPartsDestroyHeight = -200000
            
            local player = game.Players.LocalPlayer
            local character = player.Character
            if not character then
                character = player.CharacterAdded:Wait()
            end
            
            local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
            
            -- Сохраняем исходную позицию
            local originalPosition = humanoidRootPart.Position
            
            -- Телепортируем вниз
            local targetPosition = Vector3.new(
                originalPosition.X,
                -9990,
                originalPosition.Z
            )
            
            humanoidRootPart.CFrame = CFrame.new(targetPosition)
            
            -- Ждем немного и возвращаем обратно
            task.wait(0.5)
            humanoidRootPart.CFrame = CFrame.new(originalPosition)
            workspace.FallenPartsDestroyHeight = -500 -- Восстанавливаем стандартную высоту
        end
        
        if Value then
            -- Запускаем основной цикл
            spawn(function()
                while loopkick do
                    local plr = game.Players:FindFirstChild(selectedPlrName)
                    if plr then
                        -- Отслеживаем возрождение игрока
                        if connection then
                            connection:Disconnect()
                        end
                        
                        connection = plr.CharacterAdded:Connect(function()
                            if loopkick then
                                task.wait(2) -- Ждем 2 секунды после возрождения
                                kickPlayer() -- Запускаем снова
                            end
                        end)
                        
                        -- Выполняем первый раз
                        kickPlayer()
                        
                        -- Ждем пока игрок умрет или тоггл выключится
                        while loopkick and plr.Character do
                            task.wait(0.1)
                        end
                    else
                        task.wait(1)
                    end
                end
            end)
        else
            -- Отключаем соединение при выключении
            if connection then
                connection:Disconnect()
                connection = nil
            end
            -- Восстанавливаем стандартную высоту уничтожения
            workspace.FallenPartsDestroyHeight = -500
        end
   end,
})
local Button = Tab:CreateButton({
   Name = "Граб и телепорт[кнопка]",
   Callback = function()
        -- Находим игрока
        local plr = game.Players:FindFirstChild(selectedPlrName)
        if not plr then return end
        if not plr.Character then return end
        local hrp = plr.Character:FindFirstChild("HumanoidRootPart")
        if not hrp then return end
        local blob = FindBlob()
        if not blob then return end
        
        -- 1. Сначала телепортируемся к игроку
        local myCharacter = game.Players.LocalPlayer.Character
        if myCharacter then
            local myHrp = myCharacter:FindFirstChild("HumanoidRootPart")
            if myHrp then
                myHrp.CFrame = hrp.CFrame * CFrame.new(0, 3, 0) -- телепорт к игроку +3 studs выше
                task.wait(0.5) -- увеличена задержка после телепорта
            end
        end
        
        -- 2. Потом Grab игрока
        blob.BlobmanSeatAndOwnerScript.CreatureGrab:FireServer(
            blob.LeftDetector,
            hrp, 
            blob.LeftDetector.LeftWeld
        )
        
        -- Ждем немного чтобы граб зарегистрировался
        task.wait(0.5) -- увеличена задержка после граба
        
        -- 3. Телепорт ниже уровня уничтожения
        workspace.FallenPartsDestroyHeight = -200000
        
        local player = game.Players.LocalPlayer
        local character = player.Character
        if not character then
            character = player.CharacterAdded:Wait()
        end
        
        local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
        
        -- Сохраняем исходную позицию
        local originalPosition = humanoidRootPart.Position
        
        -- Телепортируем вниз
        local targetPosition = Vector3.new(
            originalPosition.X,
            -9990,
            originalPosition.Z
        )
        
        humanoidRootPart.CFrame = CFrame.new(targetPosition)
        
        -- Ждем немного и возвращаем обратно
        task.wait(0.5)
        humanoidRootPart.CFrame = CFrame.new(originalPosition)
   end,
})
local Button = Tab:CreateButton({
   Name = "прыжок grab",
   Callback = function()
        local plr = game.Players:FindFirstChild(selectedPlrName)
        if not plr then return end
        local hrp = plr.Character.HumanoidRootPart
        local blob = FindBlob()
        
        -- Один граб
        blob.BlobmanSeatAndOwnerScript.CreatureGrab:FireServer(
            blob.LeftDetector,
            hrp, 
            blob.LeftDetector.LeftWeld
        )
        
        -- Немного ждем чтобы граб зарегистрировался
        task.wait(0.1)
        
        -- Заставляем прыгнуть и blobmen и нашего персонажа
        local ourCharacter = game.Players.LocalPlayer.Character
        if ourCharacter then
            local ourHumanoid = ourCharacter:FindFirstChild("Humanoid")
            if ourHumanoid then
                ourHumanoid:ChangeState(Enum.HumanoidStateType.Jumping)
            end
            
            -- Также применяем силу к нашему персонажу
            local ourRoot = ourCharacter:FindFirstChild("HumanoidRootPart")
            if ourRoot then
                local bodyVelocity = Instance.new("BodyVelocity")
                bodyVelocity.Velocity = Vector3.new(0, 50, 0)
                bodyVelocity.MaxForce = Vector3.new(0, 5000, 0)
                bodyVelocity.Parent = ourRoot
                
                task.delay(0.3, function()
                    bodyVelocity:Destroy()
                end)
            end
        end
        
        -- Также применяем силу к blobmen
        local blobPrimary = blob.PrimaryPart or blob:FindFirstChildWhichIsA("BasePart")
        if blobPrimary then
            local blobVelocity = Instance.new("BodyVelocity")
            blobVelocity.Velocity = Vector3.new(0, 50, 0)
            blobVelocity.MaxForce = Vector3.new(0, 5000, 0)
            blobVelocity.Parent = blobPrimary
            
            task.delay(0.3, function()
                blobVelocity:Destroy()
            end)
        end
   end,
})
local Button = Tab:CreateButton({
   Name = "sit grab",
   Callback = function()
        local plr = game.Players:FindFirstChild(selectedPlrName)
        if not plr then return end
        local hrp = plr.Character.HumanoidRootPart
        local blob = FindBlob()
        

        blob.BlobmanSeatAndOwnerScript.CreatureGrab:FireServer(
            blob.LeftDetector,
            hrp, 
            blob.LeftDetector.LeftWeld
        )
        

        task.wait(0.1)
        

        local ourCharacter = game.Players.LocalPlayer.Character
        if ourCharacter then
            local ourHumanoid = ourCharacter:FindFirstChild("Humanoid")
            if ourHumanoid then
                ourHumanoid:ChangeState(Enum.HumanoidStateType.Jumping)
            end
            

            local ourRoot = ourCharacter:FindFirstChild("HumanoidRootPart")
            if ourRoot then
                local bodyVelocity = Instance.new("BodyVelocity")
                bodyVelocity.Velocity = Vector3.new(0, 50, 0)
                bodyVelocity.MaxForce = Vector3.new(0, 5000, 0)
                bodyVelocity.Parent = ourRoot
                
                task.delay(0.3, function()
                    bodyVelocity:Destroy()
                    

                    task.wait(0.1)
                    

                    local blobSeat = blob:FindFirstChild("Seat") or blob:FindFirstChildWhichIsA("VehicleSeat")
                    if blobSeat and ourCharacter:FindFirstChild("Humanoid") then

                        ourCharacter.Humanoid.Sit = true
                        ourRoot.CFrame = blobSeat.CFrame * CFrame.new(0, 2, 0)
                    end
                end)
            end
        end
        
        local blobPrimary = blob.PrimaryPart or blob:FindFirstChildWhichIsA("BasePart")
        if blobPrimary then
            local blobVelocity = Instance.new("BodyVelocity")
            blobVelocity.Velocity = Vector3.new(0, 50, 0)
            blobVelocity.MaxForce = Vector3.new(0, 5000, 0)
            blobVelocity.Parent = blobPrimary
            
            task.delay(0.3, function()
                blobVelocity:Destroy()
            end)
        end
   end,
})
