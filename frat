local Button = Tab:CreateButton({
   Name = "Grab and Drop",
   Callback = function()
        local plr = game.Players:FindFirstChild(selectedPlrName)
        if not plr then return end
        local hrp = plr.Character.HumanoidRootPart
        local blob = FindBlob()
        
        -- Один граб
        blob.BlobmanSeatAndOwnerScript.CreatureGrab:FireServer(
            blob.LeftDetector,
            hrp, 
            blob.LeftDetector.LeftWeld
        )
        
        task.wait(-0.00386) -- небольшая задержка между грабом и дропом
        
        -- Один дроп
        blob.BlobmanSeatAndOwnerScript.CreatureDrop:FireServer(
            blob.LeftDetector.LeftWeld,
            hrp
        )
   end,
})

local Button = Tab:CreateButton({
   Name = "клик снос",
   Callback = function()
        -- Находим игрока
        local plr = game.Players:FindFirstChild(selectedPlrName)
        if not plr then return end
        if not plr.Character then return end
        
        local hrp = plr.Character:FindFirstChild("HumanoidRootPart")
        if not hrp then return end
        
        local blob = FindBlob()
        if not blob then return end
        
        -- Грабим игрока
        blob.BlobmanSeatAndOwnerScript.CreatureGrab:FireServer(
            blob.LeftDetector,
            hrp, 
            blob.LeftDetector.LeftWeld
        )
        
        task.wait(-0.00386)
        
        -- Дропаем игрока
        blob.BlobmanSeatAndOwnerScript.CreatureDrop:FireServer(
            blob.LeftDetector.LeftWeld,
            hrp
        )

        task.wait(-0.00386)
        -- Запускаем игрока вверх на 500000
        hrp.AssemblyLinearVelocity = Vector3.new(0, 500000, 0)
        
        print("Игрок запущен вверх!")
   end,
})

local Toggle = Tab:CreateToggle({
   Name = "снос",
   CurrentValue = false,
   Flag = "Toggle1",
   Callback = function(Value)
    loopkick = Value
    local plr = game.Players:FindFirstChild(selectedPlrName)
    if not plr then return end
    if not plr.Character then return end
    local hrp = plr.Character:FindFirstChild("HumanoidRootPart")
    if not hrp then return end
    local blob = FindBlob()
    if not blob then return end
    
    -- 2. Взлетаем на blobmen на 50
    local myCharacter = game.Players.LocalPlayer.Character
    if myCharacter then
        local myHrp = myCharacter:FindFirstChild("HumanoidRootPart")
        local blobPrimary = blob.PrimaryPart or blob:FindFirstChildWhichIsA("BasePart")
        
        if myHrp and blobPrimary then
            -- Взлетаем вместе с blobmen
            myHrp.AssemblyLinearVelocity = Vector3.new(0, 70, 0)
            blobPrimary.AssemblyLinearVelocity = Vector3.new(0, 70, 0)
            task.wait(0.2) -- ждем взлета
        end
    end
    
    -- 1. Сначала Grab
    blob.BlobmanSeatAndOwnerScript.CreatureGrab:FireServer(
        blob.LeftDetector,
        hrp, 
        blob.LeftDetector.LeftWeld
    )
task.wait(0.2)
    -- 3. Потом Drop
    blob.BlobmanSeatAndOwnerScript.CreatureDrop:FireServer(
        blob.LeftDetector.LeftWeld,
        hrp
    )
task.wait(1)
	    blob.BlobmanSeatAndOwnerScript.CreatureGrab:FireServer(
        blob.LeftDetector,
        hrp, 
        blob.LeftDetector.LeftWeld
    )
    task.wait(0.2)
	    blob.BlobmanSeatAndOwnerScript.CreatureDrop:FireServer(
        blob.LeftDetector.LeftWeld,
        hrp
    )
	 -- ждем немного
    
    -- 4. Дальше Grab, Drop и запуск игрока в цикле
    while loopkick do
        -- Грабим игрока
        blob.BlobmanSeatAndOwnerScript.CreatureGrab:FireServer(
            blob.LeftDetector,
            hrp, 
            blob.LeftDetector.LeftWeld
        )
        task.wait(0.03)
        
        -- Дропаем игрока
        blob.BlobmanSeatAndOwnerScript.CreatureDrop:FireServer(
            blob.LeftDetector.LeftWeld,
            hrp
        )
        task.wait(0.03)
        
        -- Запускаем игрока вверх
        hrp.AssemblyLinearVelocity = Vector3.new(0, 500000, 0)
    end
   end,
})

Tab:CreateToggle({
   Name = "LoopKick",
   CurrentValue = false,
   Flag = "Toggle1",
   Callback = function(Value)
    loopkick = Value
    local plr = game.Players:FindFirstChild(selectedPlrName)
    if not plr then return end
    if not plr.Character then return end
    local hrp = plr.Character:FindFirstChild("HumanoidRootPart")
    if not hrp then return end
    local blob = FindBlob()
    if not blob then return end
    
    
    task.wait(0.1) -- ждем немного
    
    -- 2. Взлетаем на blobmen на 50
    local myCharacter = game.Players.LocalPlayer.Character
    if myCharacter then
        local myHrp = myCharacter:FindFirstChild("HumanoidRootPart")
        local blobPrimary = blob.PrimaryPart or blob:FindFirstChildWhichIsA("BasePart")
        
        if myHrp and blobPrimary then
            -- Взлетаем вместе с blobmen
            myHrp.AssemblyLinearVelocity = Vector3.new(0, 50, 0)
            blobPrimary.AssemblyLinearVelocity = Vector3.new(0, 50, 0)
            task.wait(0.2) -- ждем взлета
        end
    end
    
    -- 1. Сначала Grab
    blob.BlobmanSeatAndOwnerScript.CreatureGrab:FireServer(
        blob.LeftDetector,
        hrp, 
        blob.LeftDetector.LeftWeld
    )
task.wait(0.2)
    -- 3. Потом Drop
    blob.BlobmanSeatAndOwnerScript.CreatureDrop:FireServer(
        blob.LeftDetector.LeftWeld,
        hrp
    )
task.wait(1)
	    blob.BlobmanSeatAndOwnerScript.CreatureGrab:FireServer(
        blob.LeftDetector,
        hrp, 
        blob.LeftDetector.LeftWeld
    )

	    blob.BlobmanSeatAndOwnerScript.CreatureDrop:FireServer(
        blob.LeftDetector.LeftWeld,
        hrp
    )

    
    
    -- 4. Дальше Grab и Drop в цикле
    while loopkick do
        -- Грабим игрока
        blob.BlobmanSeatAndOwnerScript.CreatureGrab:FireServer(
            blob.LeftDetector,
            hrp, 
            blob.LeftDetector.LeftWeld
        )
        task.wait(-0.00386)
        
        -- Дропаем игрока
        blob.BlobmanSeatAndOwnerScript.CreatureDrop:FireServer(
            blob.LeftDetector.LeftWeld,
            hrp
        )
        task.wait(-0.00386)
    end
   end,
})
game.Players.PlayerAdded:Connect(function()
Dropdown1:Refresh(GetPlayers())
end)

game.Players.PlayerRemoving:Connect(function()
Dropdown1:Refresh(GetPlayers())
end)
local Tab = Window:CreateTab("куни", 4483362458) -- Title, Image
local AntiDeath = false
Tab:CreateToggle({
   Name = "anti-fly [DEATH]",
   CurrentValue = false,
   Flag = "Toggle123", 
   Callback = function(Value)
    AntiDeath = Value
   end,
})
task.spawn(function()
game:GetService("ReplicatedStorage").GameCorrectionEvents.TeleportToGround.OnClientEvent:Connect(function()
    if AntiDeath then 
        game.Players.LocalPlayer.Character.Humanoid.Health = 0
    end
end)
end)
local Toggle = Tab:CreateToggle({
   Name = "anti-blob",
   CurrentValue = false,
   Flag = "Toggle1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Value)
          if Value then
           for _, v in pairs(workspace:GetDescendants()) do
               if v.Name == "LeftWeld" then
                   v.Enabled = false
               elseif v.Name == "LeftAlignOrientation" then
                   v.Enabled = false
               elseif v.Name == "RigidConstraint" then
                   v.Enabled = false
               elseif v.Name == "RightWeld" then
                   v.Enabled = false
               elseif v.Name == "RightAlignOrientation" then
                   v.Enabled = false
               end
           end
                      if ablob then
               ablob:Disconnect()
           end
                      ablob = workspace.DescendantAdded:Connect(function(v)
               if v.Name == "LeftWeld" then
                   v.Enabled = false
               elseif v.Name == "RigidConstraint" then
                   v.Enabled = false
               elseif v.Name == "LeftAlignOrientation" then
                   v.Enabled = false
               elseif v.Name == "RightWeld" then
                   v.Enabled = false
               elseif v.Name == "RightAlignOrientation" then
                   v.Enabled = false
               end
           end)
       else
           if ablob then
               ablob:Disconnect()
               ablob = nil
           end
        	           for _, v in pairs(workspace:GetDescendants()) do
               if v.Name == "LeftWeld" then
                   v.Enabled = true
               elseif v.Name == "LeftAlignOrientation" then
                   v.Enabled = true
               elseif v.Name == "RigidConstraint" then
                   v.Enabled = true
               elseif v.Name == "RightWeld" then
                   v.Enabled = true
               elseif v.Name == "RightAlignOrientation" then
                   v.Enabled = true
               end
           end
       end
   end,
})
local Sense = 30

local Massless
local Toggle = Tab:CreateToggle({
   Name = "Massless Grab",
   CurrentValue = false,
   Flag = "Toggle1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Value)
if Value then
Massless = game.Workspace.ChildAdded:Connect(function(r)
if r.Name == "GrabParts" then
while game.Workspace:FindFirstChild("GrabParts") do
wait(0)
r.DragPart.AlignPosition.Responsiveness = Sense
r.DragPart.AlignOrientation.Responsiveness = Sense
r.DragPart.AlignPosition.MaxForce = math.huge
r.DragPart.AlignPosition.MaxVelocity = math.huge
r.DragPart.AlignOrientation.MaxTorque = math.huge
end
end
 end)
else
Massless:Disconnect()
Massless = nil
end
   end,
})
